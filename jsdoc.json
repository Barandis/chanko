{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "packages/csp/src",
      "packages/ops/src",
      "packages/xduce-tools/src",
      "packages/xduce/src"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "docs/api/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true
  }
}
